{
  "name": "internal-data-portal",
  "private": true,
  "version": "0.1.0",
  "description": "DataPortal is envisioned as a comprehensive data management system, designed to facilitate easy access, manipulation, and visualization of datasets.",
  "main": "src/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rm -rf ./dist",
    "start": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "ts-node": "ts-node -r tsconfig-paths/register --logError",
    "migrate": "ts-node -r tsconfig-paths/register ./bin/migrate.ts",
    "seed": "ts-node -r tsconfig-paths/register ./bin/seed.ts",
    "test": "jest --watchAll --runInBand --detectOpenHandles"
  },
  "author": "Michael Johnson",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.0",
    "axios": "^1.6.5",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "helmet": "^7.1.0",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "sequelize": "^6.35.2",
    "slugify": "^1.6.6",
    "tedious": "^16.6.1",
    "umzug": "^3.5.0"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
